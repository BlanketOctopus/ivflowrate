{
  "name": "ivflowrate",
  "version": "1.0.0",
  "description": "IV Flow Rate Calculator PWA",
  "scripts": {
    "build": "npx babel components --out-dir components --out-file-extension .min.js",
    "watch": "npx babel components --out-dir components --watch --out-file-extension .min.js"
  },
  "repository": "github:blanketoctopus/ivflowrate",
  "author": "BlanketOctopus",
  "license": "Unlicense",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "babel-preset-minify": "^0.5.1"
  },
  "babel": {
    "comments": false,
    "presets": [
      "@babel/preset-react",
      "minify"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
